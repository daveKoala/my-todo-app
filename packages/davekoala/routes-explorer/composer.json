{
    "name": "davekoala/routes-explorer",
    "description": "Laravel package for analyzing routes and their complete dependency chains",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DaveKoala\\RoutesExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DaveKoala\\RoutesExplorer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DaveKoala\\RoutesExplorer\\RoutesExplorerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
